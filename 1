<script>

  function f1() {
    var sir = [3, 8, 9, 5, 4, 0, 1, 2, 7];

    document.getElementById("displayer").innerHTML += "Sir:";
    document.getElementById("displayer").innerHTML += sir.join(', ') + "<br>";

   
    var ordinecresc= sir.slice().sort(function(a, b) {
      return a - b;
    });
    var ordinedesc = sir.slice().sort(function(a, b) {
      return b - a;
    });

    document.getElementById("displayer").innerHTML += "Ordonare crescatoare:";
    document.getElementById("displayer").innerHTML += ordinecresc.join(', ') + "<br>";

    document.getElementById("displayer").innerHTML += "Ordonarea descrescatoare:";
    document.getElementById("displayer").innerHTML += ordinedesc.join(', ') + "<br>";


    var nrimpare = sir.filter(function(n) {
      return n % 2 !== 0;
    });

    document.getElementById("displayer").innerHTML += "Sirul de mai sus fara valorile pare:";
    document.getElementById("displayer").innerHTML += nrimpare.join(', ') + "<br>";

    var valintroduse = [24,76,98];
    sir = sir.concat(valintroduse);

    document.getElementById("displayer").innerHTML += "Sirul cu valorile adaugate:";
    document.getElementById("displayer").innerHTML += sir.join(', ') + "<br>";
  }
</script>

</head>

<body onLoad="f1()">

<div id="displayer">
</div>


</body>
